declare A
  a1 : int
  a2 : int
end

declare B
  b1 : int
  b2 : int
end

declare C
  c1 : int
  c2 : int
end

declare D
  d1 : int
  d2 : int
end

declare E
  e1 : int
  e2 : int
end

rule Init salience 100
when
then
    for (int i = 0; i < 1000000; i++) {
        insert( new A(1, 1) );
    }
    insert( new B(0, 2) );
    insert( new C(-1, 2) );
    insert( new D(-2, 2) );
    insert( new E(-3, 2) );
end

rule R1
when
    $a: A( a1 < 10 )
    $b: B( b1 < $a.a1 )
    $c: C( c1 < $b.b1 )
    $d: D( d1 < $c.c1 )
    $e: E( e1 < $d.d1, e2 > $a.a2 )
then
    modify( $a ) { setA2( $e.getE2() + 1 ) };
end
